<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.cdkj.token">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        tools:replace="android:allowBackup">
        <activity android:name=".user.login.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".wallet.account.BillActivity" />
        <activity android:name=".wallet.account.BillDetailActivity" />
        <activity android:name=".wallet.account.RechargeActivity" />
        <activity android:name=".wallet.account.WithdrawActivity" />
        <activity android:name=".user.UserSettingActivity" />
        <activity android:name=".user.UserEmailActivity" />
        <activity android:name=".user.UserLanguageActivity" />
        <activity android:name=".user.UserPasswordActivity" />
        <activity android:name=".user.UserAboutActivity" />
        <activity
            android:name=".user.login.SignInActivity"
            android:launchMode="singleTask"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".user.login.SignUpActivity" />
        //找回密码
        <activity
            android:name=".user.login.FindPwdActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        //修改手机号
        <activity
            android:name="com.cdkj.baselibrary.activitys.UpdatePhoneActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        //资金密码修改
        <activity
            android:name="com.cdkj.baselibrary.activitys.PayPwdModifyActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        //H5
        <activity
            android:name="com.cdkj.baselibrary.activitys.WebViewActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        // 图片
        <activity
            android:name="com.cdkj.baselibrary.activitys.ImageSelectActivity"
            android:theme="@style/ActivityDialogStyleBottom" />
        // 实名认证
        <activity
            android:name="com.cdkj.baselibrary.activitys.AuthenticateActivity"
            android:launchMode="singleTask">

            // 需要添加下面的intent-filter配置
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                //scheme:为 实名认证Activity，认证接口参数"returnUrl"冒号前字符，host为同位置参数冒号后字符
                <data
                    android:host="certify.back"
                    android:scheme="orangecoin" />
            </intent-filter>
        </activity>
        <activity android:name=".consult.ConsultActivity" />
        <activity android:name=".consult.StatisticsActivity" />
        <activity android:name=".wallet.account.WithdrawOrderActivity" />
        <activity android:name=".user.UserGoogleActivity" />

        <!--拨打电话-->
        <activity
            android:name=".user.CallPhoneActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ActivityDialogStyleBottom"
            android:windowSoftInputMode="adjustPan" />
        <!--店铺支付-->
        <activity
            android:name=".consult.StorePayActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".consult.ConsultListActivity" />
        <activity android:name=".consult.NoneActivity" />
        <activity android:name=".wallet.account.BillListActivity" />
        <activity android:name=".wallet.AddChoiceCoinActivity" />
        <activity android:name=".consult.MsgListActivity" />
        <activity android:name=".user.UserJoinActivity" />
        <activity android:name=".user.UserWalletActivity" />

        <service android:name=".service.CoinListService" />

        <activity android:name=".wallet.IntoWalletBeforeActivity" />
        <activity android:name=".wallet.create_guide.CreatePassWordActivity" />
        <activity android:name=".wallet.create_guide.CreateWalletSuccessActivity" />
        <activity android:name=".wallet.create_guide.WalletBackupCheckActivity" />
        <activity android:name=".wallet.create_guide.WalletHelpWordsShowActivity" />
        <activity android:name=".wallet.import_guide.WalletImportWordsInputActivity" />
        <activity android:name=".wallet.import_guide.ImportWalletSuccessActivity" />
        <activity android:name=".wallet.import_guide.ImportCreatePassWordActivity" />
        <activity android:name=".wallet.backup_guide.WalletBackupStartActivity" />
        <activity android:name=".wallet.WalletPasswordModifyActivity" />
        <activity android:name=".wallet.backup_guide.WalletBackupPasswordCheckActivity" />
        <activity android:name=".wallet.delete_guide.WallteDeleteStartActivity" />
        <activity android:name=".wallet.delete_guide.WalletDeleteWordsInputActivity" />
        <activity android:name=".wallet.export.CoinKeySelectActivity" />
        <activity android:name=".wallet.export.CoinPrivateKeyShowActivity" />
        <activity android:name=".wallet.export.WalletExportPasswordCheckActivity" />
        <activity android:name=".wallet.coin_detail.WalletCoinDetailsActivity" />
        <activity android:name=".wallet.coin_detail.WalletAddressShowActivity" />
        <activity android:name=".wallet.coin_detail.WalletTransferActivity" />
        <activity android:name=".wallet.trusteeship.WalletUserActivity" />
        <activity android:name=".wallet.AddChoiceActivity2" />
        <activity android:name=".wallet.coin_detail.TransactionDetailsActivity" />
        <activity android:name=".wallet.red_package.SendRedPackageActivity" />
        <activity android:name=".wallet.red_package.RedPackageShearActivity" />
        <activity android:name=".wallet.red_package.RedPackageHistoryActivity" />
        <activity android:name=".user.CountryCodeListActivity" />
        <activity
            android:name=".wallet.red_package.QRRedPackageImgActivity"
            android:theme="@style/ActivityDialogStyleBottom"></activity>
        <activity android:name=".user.LocalCoinTypeChooseActivity" />
    </application>

</manifest>